<?xml version="1.0"?>
<transfer
    xmlns="http://xsd.spryker.com"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd"
>

    <transfer name="FactFinderApiSearchRequest">
        <property name="query" type="string"/>
        <property name="page" type="string"/>
        <property name="sortName" type="string"/>
        <property name="sortPrice" type="string"/>
        <property name="format" type="string"/>
        <property name="channel" type="string"/>
        <property name="productsPerPage" type="int"/>
        <property name="sort" type="string"/>
        <property name="filter" type="string"/>
        <property name="searchField" type="string"/>
        <property name="noArticleNumberSearch" type="bool"/>
        <property name="sid" type="string"/>
        <property name="useAsn" type="bool"/>
        <property name="useFoundWords" type="string"/>
        <property name="useCampaigns" type="bool"/>
        <property name="navigation" type="string"/>
        <property name="idsOnly" type="bool"/>
        <property name="useKeywords" type="bool"/>
        <property name="generateAdvisorTree" type="bool"/>
        <property name="disableCache" type="bool"/>
        <property name="followSearch" type="bool"/>
        <property name="usePersonalization" type="bool"/>
        <property name="useSemanticEnhancer" type="bool"/>
        <property name="useAso" type="bool"/>
    </transfer>

    <transfer name="FactFinderApiSearchResponse">
        <property name="campaignIterator" type="FactFinderApiDataCampaignIterator"/>
        <property name="afterSearchNavigation" type="FactFinderApiDataAfterSearchNavigation"/>
        <property name="breadCrumbs" type="FactFinderApiDataBreadCrumb[]"/>
        <property name="paging" type="FactFinderApiDataPaging"/>
        <property name="result" type="FactFinderApiDataResult"/>
        <property name="resultsPerPageOptions" type="FactFinderApiDataResultsPerPageOptions"/>
        <property name="singleWordSearchItems" type="FactFinderApiDataSingleWordSearchItem[]"/>
        <property name="sortingItems" type="FactFinderApiDataItem[]"/>
        <property name="isSearchTimedOut" type="bool"/>
        <property name="followSearchValue" type="int"/>
    </transfer>

    <!--FACTFinder\Data\.. Objects-->
    <transfer name="FactFinderApiDataItem">
        <property name="label" type="string"/>
        <property name="url" type="string"/>
        <property name="selected" type="bool"/>
    </transfer>

    <transfer name="FactFinderApiDataCampaignIterator">
        <property name="hasRedirect" type="bool"/>
        <property name="redirectUrl" type="string"/>
        <property name="hasFeedback" type="bool"/>
        <property name="feedback" type="string"/>
        <property name="hasPushedProducts" type="bool"/>
        <property name="pushedProducts" type="FactFinderApiDataRecord[]"/>
        <property name="hasActiveQuestions" type="bool"/>
        <property name="getActiveQuestions" type="FactFinderApiDataRecord[]"/>
        <property name="hasAdvisorTree" type="bool"/>
        <property name="advisorTree" type="FactFinderApiDataRecord[]"/>

        <property name="campaigns" type="FactFinderApiDataCampaign[]"/>
    </transfer>

    <transfer name="FactFinderApiDataCampaign">
        <property name="name" type="string"/>
        <property name="category" type="string"/>
        <property name="redirectUrl" type="string"/>
        <property name="pushedProducts" type="FactFinderApiDataRecord[]"/>
        <property name="feedback" type="array"/>
        <property name="activeQuestions" type="FactFinderApiDataAdvisorQuestion[]"/>
        <property name="advisorTree" type="FactFinderApiDataAdvisorQuestion[]"/>
        <property name="hasRedirect" type="bool"/>
    </transfer>

    <transfer name="FactFinderApiDataAfterSearchNavigation">
        <property name="hasPreviewImages" type="bool"/>

        <property name="filterGroups" type="FactFinderApiDataFilterGroup[]"/>
    </transfer>

    <transfer name="FactFinderApiDataFilterGroup">
        <property name="filters" type="FactFinderApiDataFilter[]"/>

        <property name="name" type="string"/>
        <property name="detailedLinkCount" type="int"/>
        <property name="unit" type="string"/>
        <property name="isRegularStyle" type="bool"/>
        <property name="isSliderStyle" type="bool"/>
        <property name="isTreeStyle" type="bool"/>
        <property name="isMultiSelectStyle" type="bool"/>
        <property name="hasPreviewImages" type="bool"/>
        <property name="hasSelectedItems" type="bool"/>
        <property name="isSingleHideUnselectedType" type="bool"/>
        <property name="isSingleShowUnselectedType" type="bool"/>
        <property name="isMultiSelectOrType" type="bool"/>
        <property name="isMultiSelectAndType" type="bool"/>
        <property name="isTextType" type="bool"/>
        <property name="isNumberType" type="bool"/>
    </transfer>

    <transfer name="FactFinderApiDataFilter">
        <property name="item" type="FactFinderApiDataItem"/>

        <property name="fieldName" type="string"/>
        <property name="matchCount" type="int"/>
        <property name="clusterLevel" type="int"/>
        <property name="previewImage" type="string"/>
        <property name="hasPreviewImage" type="bool"/>
    </transfer>

    <transfer name="FactFinderApiDataBreadCrumb">
        <property name="item" type="FactFinderApiDataItem"/>

        <property name="isSearchBreadCrumb" type="bool"/>
        <property name="isFilterBreadCrumb" type="bool"/>
        <property name="fieldName" type="string"/>
    </transfer>

    <transfer name="FactFinderApiDataPaging">
        <property name="pageCount" type="int"/>
        <property name="firstPage" type="FactFinderApiDataPage"/>
        <property name="lastPage" type="FactFinderApiDataPage"/>
        <property name="previousPage" type="FactFinderApiDataPage"/>
        <property name="currentPage" type="FactFinderApiDataPage"/>
        <property name="nextPage" type="FactFinderApiDataPage"/>
    </transfer>

    <transfer name="FactFinderApiDataPage">
        <property name="item" type="FactFinderApiDataItem"/>
        <property name="pageNumber" type="int"/>
    </transfer>

    <transfer name="FactFinderApiDataRecord">
        <property name="id" type="string"/>
        <property name="fields" type="array"/>
        <property name="similarity" type="float"/>
        <property name="position" type="int"/>
        <property name="seoPath" type="string"/>
        <property name="keywords" type="array"/>
    </transfer>

    <transfer name="FactFinderApiDataAdvisorQuestion">
        <property name="advisorAnswers" type="FactFinderApiDataAdvisorAnswer[]"/>
        <property name="text" type="string"/>
        <property name="hasAnswers" type="bool"/>
    </transfer>

    <transfer name="FactFinderApiDataAdvisorAnswer">
        <property name="item" type="FactFinderApiDataItem"/>
        <property name="text" type="string"/>
        <property name="followUpQuestions" type="FactFinderApiDataAdvisorQuestion[]"/>
    </transfer>

    <transfer name="FactFinderApiDataResult">
        <property name="records" type="FactFinderApiDataRecord[]"/>
        <property name="fieldNames" type="array"/>
        <property name="foundRecordsCount" type="int"/>
    </transfer>

    <transfer name="FactFinderApiDataResultsPerPageOptions">
        <property name="items" type="FactFinderApiDataItem[]"/>
        <property name="defaultOption" type="FactFinderApiDataItem"/>
        <property name="selectedOption" type="FactFinderApiDataItem"/>
    </transfer>

    <transfer name="FactFinderApiDataSuggestQuery">
        <property name="item" type="FactFinderApiDataItem"/>
        <property name="hitCount" type="int"/>
        <property name="type" type="string"/>
        <property name="imageUrl" type="string"/>
        <property name="attributes" type="array"/>
    </transfer>

    <transfer name="FactFinderApiDataSingleWordSearchItem">
        <property name="suggestQuery" type="FactFinderApiDataSuggestQuery"/>
        <property name="previewRecords" type="FactFinderApiDataRecord[]"/>
    </transfer>

    <!--END FACTFinder\Data\.. Objects-->

    <transfer name="FactFinderApiRecommendationRequest">
        <property name="id" type="string"/>
    </transfer>

    <transfer name="FactFinderApiRecommendationResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderApiSuggestRequest">
        <property name="query" type="string"/>
    </transfer>

    <transfer name="FactFinderApiSuggestResponse">
        <property name="suggestions" type="array"/>
    </transfer>

    <transfer name="FactFinderApiTagCloudRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderApiTagCloudResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderApiTrackingRequest">
        <property name="id" type="string"/>ยง
        <property name="masterId" type="string"/>
        <property name="channel" type="string"/>
        <property name="sid" type="string"/>
        <property name="event" type="string"/>
        <property name="title" type="string"/>
        <property name="userId" type="string"/>
        <property name="cookieId" type="string"/>
        <property name="query" type="string"/>
        <property name="pos" type="int"/>
        <property name="origPos" type="int"/>
        <property name="page" type="int"/>
        <property name="pageSize" type="int"/>
        <property name="origPageSize" type="int"/>
        <property name="simi" type="string"/>
        <property name="campaign" type="string"/>
        <property name="count" type="int"/>
        <property name="price" type="string"/>
        <property name="mainId" type="string"/>
        <property name="positive" type="bool"/>
        <property name="message" type="string"/>
        <property name="hitCount" type="int"/>
        <property name="searchTime" type="string"/>
        <property name="bestSimi" type="string"/>
        <property name="minSimi" type="string"/>
        <property name="filterFieldName" type="string"/>
        <property name="searchField" type="string"/>
        <property name="customSorting" type="string"/>
        <property name="additionalInfo" type="string"/>
        <property name="queryFromSuggest" type="string"/>
        <property name="userInput" type="string"/>
    </transfer>

    <transfer name="FactFinderApiTrackingResponse">
        <property name="result" type="bool"/>
    </transfer>

    <transfer name="FactFinderApiSimilarRecordsRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderApiSimilarRecordsResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderApiProductCampaignRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderApiProductCampaignResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderApiCsv">
        <property name="locale" type="string" />
        <property name="type" type="string" />
        <property name="number" type="string" />
        <property name="contents" type="string" />
    </transfer>

</transfer>
